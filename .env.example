# I-9 Document Processing System - Environment Configuration
# Copy this file to .env and update with your actual values

# API Configuration
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.ai.it.cornell.edu/
I9_MODEL_NAME=google.gemini-2.5-pro

# Processing Configuration
I9_MAX_WORKERS=4
I9_RATE_LIMIT_DELAY=1.0
I9_MAX_RETRIES=3
I9_TIMEOUT_SECONDS=60

# PDF Processing
I9_IMAGE_SCALE_FACTOR=3.0
I9_MAX_PDF_SIZE_MB=50

# Detection Configuration
I9_CONFIDENCE_THRESHOLD=0.7
I9_ENABLE_FALLBACK=true

# Logging Configuration
I9_LOG_LEVEL=INFO
I9_LOG_FILE=logs/i9_processor.log
